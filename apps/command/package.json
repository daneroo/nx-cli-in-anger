{
  "name": "@nx-cli-in-anger/command",
  "version": "1.0.0",
  "description": "A TypeScript CLI app",
  "type": "module",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": "./dist/main.js"
  },
  "files": [
    "dist",
    "!dist/.tsbuildinfo",
    "bin"
  ],
  "bin": {
    "cli-in-anger": "./bin/command.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "pnpm tsc --build .",
    "start": "node dist/main.js",
    "pkgNotWorking": "pnpm build && pnpm dlx pkg . -o bin/app -t node16-linux-x64,node16-macos-x64,node16-win-x64"
  },
  "author": "Daniel Lauzon",
  "license": "ISC",
  "dependencies": {
    "@nx-cli-in-anger/shared": "workspace:*",
    "@root/walk": "^1.1.0",
    "chalk": "^5.1.0",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.13"
  }
}